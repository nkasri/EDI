<?xml version="1.0" encoding="utf-8"?>

<odoo>

        <record id="view_warning_wizard" model="ir.ui.view">

            <field name="name">warning.wizard</field>

            <field name="model">edi.warning_wizard</field>

            <field name="arch" type="xml">

            <form string="SFTP Connection ">

                <group>

                    <field name="message" />

                </group>

                <footer>

                    <button string="OK" special="cancel" class="oe_highlight"/>

                </footer>

            </form>

            </field>

        </record>
    <record id="view_warning_wizard" model="ir.ui.view">

            <field name="name">Price list fields configuration</field>

            <field name="model">edi.price_list_fields</field>

            <field name="arch" type="xml">

            <form string="Price list fields configuration ">


                        <group>
                                    <group>
                                            <field name="config"/>
                                    </group>
                                    <group>
                                            <field name="qtySupplier"/>
                                    </group>
                                    <group>
                                            <field name="priceSupplier"/>
                                    </group>
                                    <group>
                                            <field name="product_codeSupplier"/>
                                    </group>

                            </group>

                <footer>

                    <button name="edit" string="OK" type="object" class="oe_highlight" groups="base.group_user"/>

                </footer>

            </form>

            </field>

        </record>

</odoo>